{
  "$schema": "https://railway.app/railway.schema.json",
  "build": {
    "builder": "DOCKERFILE",
    "dockerfilePath": "Dockerfile"
  },
  "deploy": {
    "startCommand": "uvicorn agent.server:app --host=0.0.0.0 --port=$PORT --proxy-headers",
    "healthcheckPath": "/docs",
    "healthcheckTimeout": 100
  }
}